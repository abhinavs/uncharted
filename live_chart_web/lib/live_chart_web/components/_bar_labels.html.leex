<svg id="<%= svg_id(@chart, "ylabels") %>" class="bar__y-labels" width="8%" height="92%" y="0" x="0">
  <%= for %LiveChart.BarChart.Bar{label: label, height: height, offset: offset} <- LiveChart.BarChart.bars(@chart) do %>
    <svg x="0" y="<%= offset %>%" height="<%= height %>%" width="100%">
        <svg width="100%" height="100%">
        <text x="50%" y="50%" font-size="9px" alignment-baseline="middle" text-anchor="middle"><%= label %></text>
      </svg>
    </svg>
  <% end %>
</svg>
