<svg width="100%" height="8%" y="92%" x="0%">
  <%= for grid_line <- @grid_lines do %>
    <svg x="<%= @offsetter.(grid_line) %>%" y="0%" height="100%" width="20%">
      <svg width="100%" height="100%">
        <text x="50%" y="50%" font-size="10px" alignment-baseline="middle" text-anchor="middle"><%= grid_line %></text>
      </svg>
    </svg>
  <% end %>
</svg>
